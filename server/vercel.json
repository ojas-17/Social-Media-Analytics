{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/fetch-insights",
      "dest": "server.js"
    },
    {
      "src": "/analyze-comments",
      "dest": "server.js"
    },
    {
      "src": "/",
      "dest": "server.js"
    }
  ]
}
